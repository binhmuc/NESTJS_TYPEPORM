{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint src --ext .ts --fix",
    "test": "NODE_ENV=development jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:start": "pg_ctl -D /usr/local/var/postgres start",
    "db:stop": "pg_ctl -D /usr/local/var/postgres stop -s -m fast",
    "deploy:now": "yarn prebuild && yarn build && npx now",
    "deploy:heroku": "yarn prebuild && yarn build && git push heroku master",
    "compodoc": "npx compodoc -p tsconfig.json -s",
    "docker:up": "docker-compose -p nestjs-api-boilerplate up -d",
    "docker:down": "docker-compose -p nestjs-api-boilerplate down",
    "seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config -r src/database -n typeorm.config.ts",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed -r src/database -n typeorm.config.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/database/typeorm.config.ts",
    "migration": "yarn typeorm migration:run",
    "migration:create": "yarn typeorm migration:create -n",
    "migration:revert": "yarn typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^7.3.2",
    "@nestjs/core": "^7.3.2",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/microservices": "^7.3.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.3.2",
    "@nestjs/platform-socket.io": "^7.3.2",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.0",
    "@nestjs/websockets": "^7.3.2",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "@tensorflow/tfjs": "^2.0.1",
    "@tensorflow/tfjs-node": "^2.0.1",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/geojson": "^7946.0.7",
    "@types/node": "^14.0.26",
    "aws-sdk": "^2.720.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "cloudinary": "^1.22.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.1.3",
    "faker": "^4.1.0",
    "fs": "^0.0.2",
    "helmet": "^3.23.3",
    "ioredis": "^4.17.3",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "multer-sharp-s3": "^0.2.1",
    "nestjs-easyconfig": "^2.0.2",
    "nestjsx-automapper": "^3.0.19",
    "nexmo": "^2.8.0",
    "nodemailer": "^6.4.10",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "swagger-ui-express": "^4.1.4",
    "telerivet": "^1.4.6",
    "ts-node": "^8.10.2",
    "twilio": "^3.48.1",
    "typeorm": "^0.2.25",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^3.9.7",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/testing": "^7.3.2",
    "@types/dotenv": "^8.2.0",
    "@types/ioredis": "^4.17.2",
    "@types/jest": "^26.0.7",
    "@types/multer": "^1.4.3",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-nestjs": "^1.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.3",
    "ts-loader": "^8.0.1",
    "tsconfig-paths": "^3.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint && yarn test"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
